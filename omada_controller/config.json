{
  "name": "TP-Link Omada Controller",
  "version": "latest",
  "slug": "omada_controller",
  "description": "TP-Link Omada Controller",
  "url": "https://github.com/r15ch13/ha-addons/tree/master/omada_controller",
  "startup": "services",
  "image": "r15ch13/omada-controller-{arch}",
  "arch": ["aarch64", "amd64", "armv7", "i386"],
  "init": false,
  "map": ["backup:rw", "ssl"],
  "ports": {
    "8088": 8088,
    "8043": 8043,
    "8843": 8843,
    "29810": 29810,
    "29810/udp": 29810,
    "29811": 29811,
    "29811/udp": 29811,
    "29812": 29812,
    "29812/udp": 29812,
    "29813": 29813,
    "29813/udp": 29813
  },
  "hassio_api": true,
  "options": {
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "memory_max": "int?",
    "memory_init": "int?"
  }
}
